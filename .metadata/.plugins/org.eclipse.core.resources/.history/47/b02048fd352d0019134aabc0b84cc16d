package NewPkg;
import java.util.Scanner;
class User4 {
	public static int extractMax(String s)
	{ 
		int res=0;
		if(s.matches("[0-9].*")||s.matches("[0-9].*[.]{1}[0-9]"))
		{
			res=1;
		}
		else
			res=0;
		return res;
	
	}
}


public class Vow5 
{public static void main(String[] args)
{Scanner sc = new Scanner(System.in);int n= sc.nextInt();
String[] s= new String[n];
for(int i=0;i<n;i++)
	s[i]=sc.next();
int res=User4.extractMax(s);
if(res==1)
	System.out.println("Valid");
else
	System.out.println("invalid");}}
